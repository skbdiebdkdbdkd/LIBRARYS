function Tab:CreateToggle(data)
local State = data.CurrentValue
local Name = data.Name
local Callback = data.Callback

local ToggleButton = Instance.new("TextButton")
ToggleButton.Name = "ToggleButton"
ToggleButton.TextWrapped = true
ToggleButton.BorderSizePixel = 0
ToggleButton.TextSize = 16
ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
ToggleButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
ToggleButton.Font = Enum.Font.Jura
ToggleButton.Size = UDim2.new(0.74, 0, 0.1, 0)

ToggleButton.Text = Name
ToggleButton.Parent = Options

local ToggleButtonCorner = Instance.new("UICorner")
ToggleButtonCorner.CornerRadius = UDim.new(0, 0)
ToggleButtonCorner.Parent = ToggleButton

local ToggleButtonStroke = Instance.new("UIStroke")
ToggleButtonStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
ToggleButtonStroke.Thickness = 2
ToggleButtonStroke.Color = Color3.fromRGB(85, 73, 255)
ToggleButtonStroke.Parent = ToggleButton

-- Toggle Box
local ToggleBox = Instance.new("Frame")
ToggleBox.Name = "ToggleBox"
ToggleBox.BorderSizePixel = 0
ToggleBox.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
ToggleBox.AnchorPoint = Vector2.new(0, 0.5)
ToggleBox.Size = UDim2.new(0.3, 0, 1, 0)
ToggleBox.Position = UDim2.new(1.05, 0, 0.5, 0)
ToggleBox.Parent = ToggleButton

local ToggleBoxCorner = Instance.new("UICorner")
ToggleBoxCorner.CornerRadius = UDim.new(0, 0)
ToggleBoxCorner.Parent = ToggleBox

local ToggleBoxStroke = Instance.new("UIStroke")
ToggleBoxStroke.Thickness = 2
ToggleBoxStroke.Color = Color3.fromRGB(85, 73, 255)
ToggleBoxStroke.Parent = ToggleBox

-- Toggle Ball
local ToggleBall = Instance.new("Frame")
ToggleBall.Name = "ToggleBall"
ToggleBall.BorderSizePixel = 0
ToggleBall.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ToggleBall.Size = UDim2.new(0.5, 0, 1, 0)
ToggleBall.Position = UDim2.new(0.5, 0, 0, 0)
ToggleBall.Parent = ToggleBox

local ToggleBallCorner = Instance.new("UICorner")
ToggleBallCorner.CornerRadius = UDim.new(0, 0)
ToggleBallCorner.Parent = ToggleBall

local ToggleBallStroke = Instance.new("UIStroke")
ToggleBallStroke.Thickness = 2
ToggleBallStroke.Color = Color3.fromRGB(85, 73, 255)
ToggleBallStroke.Parent = ToggleBall


ToggleButton.MouseButton1Click:Connect(function()
  State = not State
  Callback(State)
  
  if State == true then
  ToggleBall.Position = UDim2.new(0.5, 0, 0, 0)
  ToggleBall.BackgroundColor3 = Color3.new(0,1,0)
  else
    ToggleBall.Position = UDim2.new(0, 0, 0, 0)
    ToggleBall.BackgroundColor3 = Color3.new(1,0,0)
  end
  
end)
end


