InputBox.BorderSizePixel = 0
InputBox.PlaceholderText = PlaceholderText
InputBox.TextSize = 16
InputBox.TextColor3 = Color3.fromRGB(255, 255, 255)
InputBox.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
InputBox.Font = Enum.Font.Jura
InputBox.Size = UDim2.new(1, 0, 0.5, 0)
InputBox.Position = UDim2.new(0, 0, 0.5,0)
InputBox.Parent = InputFrame

local InputBoxCorner = Instance.new("UICorner")
InputBoxCorner.CornerRadius = UDim.new(0, 0)
InputBoxCorner.Parent = InputBox

local InputBoxStroke = Instance.new("UIStroke")
InputBoxStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
InputBoxStroke.Thickness = 2
InputBoxStroke.Color = Color3.fromRGB(85, 73, 255)
InputBoxStroke.Parent = InputBox

InputBox.FocusLost:Connect(function()
  local Text = tostring(InputBox.Text)
  
  if Callback then
    Callback(Text)
  end
  
end)

end

function Tab:CreateDropDown(data)
  local Name = data.Name
  local Opciones = data.Options
  local Callback = data.Callback
  
local DropDownFrame = Instance.new("Frame")
DropDownFrame.Name = "DropDownFrame"
DropDownFrame.BorderSizePixel = 0
DropDownFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
DropDownFrame.Size = UDim2.new(1, 0, 0.2, 0)
DropDownFrame.ZIndex = 10
DropDownFrame.Parent = Options

local DropDownCorner = Instance.new("UICorner")
DropDownCorner.CornerRadius = UDim.new(0, 0)
DropDownCorner.Parent = DropDownFrame

local DropDownStroke = Instance.new("UIStroke")
DropDownStroke.Thickness = 2
DropDownStroke.Color = Color3.fromRGB(85, 73, 255)
DropDownStroke.Parent = DropDownFrame

-- Dropdown Title
local DropDownTitle = Instance.new("TextLabel")
DropDownTitle.Name = "DropDownTitle"
DropDownTitle.TextWrapped = true
DropDownTitle.BorderSizePixel = 0
DropDownTitle.TextSize = 16
DropDownTitle.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
DropDownTitle.Font = Enum.Font.Jura
DropDownTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
DropDownTitle.BackgroundTransparency = 1
DropDownTitle.Size = UDim2.new(1, 0, 0.5, 0)
DropDownTitle.Text = Name
DropDownTitle.Parent = DropDownFrame

local DropDownTitleCorner = Instance.new("UICorner")
DropDownTitleCorner.CornerRadius = UDim.new(0, 0)
DropDownTitleCorner.Parent = DropDownTitle

-- Current Option Text
local CurrentOptionText = Instance.new("TextLabel")
CurrentOptionText.Name = "CurrentOptionText"
CurrentOptionText.TextWrapped = true
CurrentOptionText.BorderSizePixel = 0
CurrentOptionText.TextSize = 16
CurrentOptionText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
CurrentOptionText.Font = Enum.Font.Jura
CurrentOptionText.TextColor3 = Color3.fromRGB(255, 255, 255)
CurrentOptionText.BackgroundTransparency = 1
CurrentOptionText.Size = UDim2.new(1, 0, 0.5, 0)
CurrentOptionText.Text = ""
CurrentOptionText.Position = UDim2.new(0, 0, 0.5, 0)
CurrentOptionText.Parent = DropDownFrame

local CurrentOptionCorner = Instance.new("UICorner")
CurrentOptionCorner.CornerRadius = UDim.new(0, 0)
CurrentOptionCorner.Parent = CurrentOptionText

local CurrentOptionStroke = Instance.new("UIStroke")
CurrentOptionStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
CurrentOptionStroke.Thickness = 2
CurrentOptionStroke.Color = Color3.fromRGB(85, 73, 255)
CurrentOptionStroke.Parent = CurrentOptionText

-- Open Dropdown Button
local OpenDropDownButton = Instance.new("TextButton")
OpenDropDownButton.Name = "OpenDropDownButton"
OpenDropDownButton.BorderSizePixel = 0
OpenDropDownButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
OpenDropDownButton.ZIndex = 10
OpenDropDownButton.BackgroundTransparency = 1
OpenDropDownButton.Size = UDim2.new(1, 0, 1, 0)
OpenDropDownButton.Text = ""
OpenDropDownButton.Parent = DropDownFrame

local OpenDropDownCorner = Instance.new("UICorner")
OpenDropDownCorner.CornerRadius = UDim.new(0, 0)
OpenDropDownCorner.Parent = OpenDropDownButton

-- Dropdown Options Scroll
local DropdownOptions = Instance.new("ScrollingFrame")
DropdownOptions.Name = "DropdownOptions"
DropdownOptions.ZIndex = 10
DropdownOptions.Visible = false
DropdownOptions.ScrollingDirection = Enum.ScrollingDirection.Y
DropdownOptions.BorderSizePixel = 0
DropdownOptions.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
DropdownOptions.AutomaticCanvasSize = Enum.AutomaticSize.Y
DropdownOptions.AutomaticSize = Enum.AutomaticSize.Y
DropdownOptions.Size = UDim2.new(1, 0, 2, 0)
DropdownOptions.Position = UDim2.new(0, 0, 0.57, 0)
DropdownOptions.ScrollBarThickness = 1
DropdownOptions.Parent = DropDownFrame

local DropdownOptionsStroke = Instance.new("UIStroke")
DropdownOptionsStroke.Thickness = 2
DropdownOptionsStroke.Color = Color3.fromRGB(85, 73, 255)
DropdownOptionsStroke.Parent = DropdownOptions

local DropdownOptionsPadding = Instance.new("UIPadding")
DropdownOptionsPadding.PaddingTop = UDim.new(0.065, 0)
DropdownOptionsPadding.PaddingRight = UDim.new(0.05, 0)
DropdownOptionsPadding.PaddingLeft = UDim.new(0.05, 0)
DropdownOptionsPadding.PaddingBottom = UDim.new(0.05, 0)
DropdownOptionsPadding.Parent = DropdownOptions

local DropdownOptionsLayout = Instance.new("UIListLayout")
DropdownOptionsLayout.Padding = UDim.new(0.03, 0)
DropdownOptionsLayout.SortOrder = Enum.SortOrder.LayoutOrder
DropdownOptionsLayout.Parent = DropdownOptions

-- Dropdown Option Button
local Open = false

OpenDropDownButton.MouseButton1Click:Connect(function()
  Open = not Open
  DropdownOptions.Visible = Open
end)


for i,v in ipairs(Opciones) do
local DropdownOptionButton = Instance.new("TextButton")
DropdownOptionButton.Name = "NormalButton"
DropdownOptionButton.TextWrapped = true
DropdownOptionButton.BorderSizePixel = 0
DropdownOptionButton.TextSize = 16
DropdownOptionButton.TextScaled = true
DropdownOptionButton.TextColor3 = Color3.fromRGB(255, 255, 255)
DropdownOptionButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
DropdownOptionButton.Font = Enum.Font.Jura
DropdownOptionButton.Size = UDim2.new(1, 0, 0.1, 0)
DropdownOptionButton.Text = tostring(v)
DropdownOptionButton.Parent = DropdownOptions

local DropdownOptionCorner = Instance.new("UICorner")
DropdownOptionCorner.CornerRadius = UDim.new(0, 0)
DropdownOptionCorner.Parent = DropdownOptionButton

local DropdownOptionStroke = Instance.new("UIStroke")
DropdownOptionStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
DropdownOptionStroke.Thickness = 2
DropdownOptionStroke.Color = Color3.fromRGB(85, 73, 255)
DropdownOptionStroke.Parent = DropdownOptionButton

DropdownOptionButton.MouseButton1Click:Connect(function()
  CurrentOptionText.Text = v
  if Callback then
  Callback(v)
  end
  DropdownOptions.Visible = false
end)
end
end

function Tab:CreateSection(data)
  local Name = data.Name
  
  local Section = Instance.new("TextLabel")
Section.Name = "Section"
Section.BorderSizePixel = 0
Section.TextSize = 16
Section.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Section.Font = Enum.Font.Jura
Section.TextColor3 = Color3.fromRGB(255, 255, 255)
Section.Size = UDim2.new(1, 0, 0.1, 0)
Section.Text = tostring(Name)
Section.Parent = Options

local SectionStroke = Instance.new("UIStroke")
SectionStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
SectionStroke.Thickness = 2
SectionStroke.Color = Color3.fromRGB(85, 73, 255)
SectionStroke.Parent = Section
end

function Tab:CreateSlider(data)
  local Name = data.Name
  local Min = data.Min or 0
  local Max = data.Max or 100
  local Default = data.Default or Min
  
  
  local SliderFrame = Instance.new("Frame")
SliderFrame.Name = "SliderFrame"
SliderFrame.BorderSizePixel = 0
SliderFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
SliderFrame.Size = UDim2.new(1, 0, 0.2, 0)
SliderFrame.Parent = Options

local SliderFrameCorner = Instance.new("UICorner")
SliderFrameCorner.CornerRadius = UDim.new(0, 0)
SliderFrameCorner.Parent = SliderFrame

local SliderFrameStroke = Instance.new("UIStroke")
SliderFrameStroke.Thickness = 2
SliderFrameStroke.Color = Color3.fromRGB(85, 73, 255)
SliderFrameStroke.Parent = SliderFrame

-- Slider Title
local SliderTitle = Instance.new("TextLabel")
SliderTitle.Name = "TitleSlider"
SliderTitle.TextWrapped = true
SliderTitle.BorderSizePixel = 0
SliderTitle.TextSize = 16
SliderTitle.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
SliderTitle.Font = Enum.Font.Jura
SliderTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
SliderTitle.BackgroundTransparency = 1
SliderTitle.AnchorPoint = Vector2.new(0, 0.5)
SliderTitle.Size = UDim2.new(0.5, 0, 0.5, 0)
SliderTitle.Text = Name
SliderTitle.Position = UDim2.new(0.1, 0, 0.25, 0)
SliderTitle.Parent = SliderFrame

-- Slider Value Text
local SliderValue = Instance.new("TextLabel")
SliderValue.Name = "ValueText"
SliderValue.TextWrapped = true
SliderValue.BorderSizePixel = 0
SliderValue.TextSize = 16
SliderValue.BackgroundColor3 = Color3.fromRGB(218, 218, 218)
SliderValue.Font = Enum.Font.Jura
SliderValue.TextColor3 = Color3.fromRGB(255, 255, 255)
SliderValue.BackgroundTransparency = 1
SliderValue.AnchorPoint = Vector2.new(0, 0.5)
SliderValue.Size = UDim2.new(0.25, 0, 0.5, 0)
SliderValue.Text = ""
SliderValue.Position = UDim2.new(0.7, 0, 0.25, 0)
SliderValue.Parent = SliderFrame

-- Slider
local Slider = Instance.new("Frame")
Slider.Name = "Slider"
Slider.BorderSizePixel = 0
Slider.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Slider.AnchorPoint = Vector2.new(0.5, 0.5)
Slider.Size = UDim2.new(1, 0, 0.5, 0)
Slider.Position = UDim2.new(0.5, 0, 0.75, 0)
Slider.Parent = SliderFrame

local SliderCorner = Instance.new("UICorner")
SliderCorner.CornerRadius = UDim.new(0, 0)
SliderCorner.Parent = Slider

local SliderStroke = Instance.new("UIStroke")
SliderStroke.Thickness = 2
SliderStroke.Color = Color3.fromRGB(85, 73, 255)
SliderStroke.Parent = Slider

-- Slider Line
local SliderLine = Instance.new("Frame")
SliderLine.Name = "Line"
SliderLine.BorderSizePixel = 0
SliderLine.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
SliderLine.AnchorPoint = Vector2.new(0.5, 0.5)
SliderLine.Size = UDim2.new(0.8, 0, 0.1, 0)
SliderLine.Position = UDim2.new(0.5, 0, 0.5, 0)
SliderLine.Parent = Slider

-- Slider Ball
local SliderBall = Instance.new("TextButton")
SliderBall.Name = "Ball"
SliderBall.Text = " "
SliderBall.BorderSizePixel = 0
SliderBall.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
SliderBall.AnchorPoint = Vector2.new(0.5, 0.5)
SliderBall.Size = UDim2.new(0.1, 0, 4, 0)
SliderBall.Position = UDim2.new(0, 0, 0.5, 0)
SliderBall.Parent = SliderLine

local SliderBallCorner = Instance.new("UICorner")
SliderBallCorner.CornerRadius = UDim.new(0, 15)
SliderBallCorner.Parent = SliderBall


local Mouse = game.Players.LocalPlayer:GetMouse()
local Sliderr = SliderLine
local Fill = SliderBall


local function ValueToPercent(value)
  return math.clamp((value - Min) / (Max - Min), 0, 1)
end

-- Funci√≥n para convertir porcentaje (0-1) a valor
local function PercentToValue(percent)
  return math.floor(Min + (percent * (Max - Min)))
end

-- Establecer valor inicial
local initialPercent = ValueToPercent(Default)
Fill.Position = UDim2.new(initialPercent, 0, 0.5, 0)
SliderValue.Text = tostring(Default)

function UpdateSlider()
  local Output = math.clamp((Mouse.X - Sliderr.AbsolutePosition.X) / Sliderr.AbsoluteSize.X, 0, 1)
  local Value = PercentToValue(Output)
  
  
  Fill.Position = UDim2.new(Output,0,0.5,0)
  SliderValue.Text = tostring(Value)
  
  if data.Callback then
  data.Callback(Value)
  end
  
end

local Active = false

function ActivarSlider()
task.spawn(function()
  Active = true
  while Active do
    UpdateSlider()
    task.wait(0.001)
  end
end)
end

Fill.MouseButton1Down:Connect(ActivarSlider)

game:GetService("UserInputService").InputEnded:Connect(function(input)
  if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
    Active = false
  end
end)



end
return Tab
end

return Window
end

return Library