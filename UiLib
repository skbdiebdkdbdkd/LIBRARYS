TabTopBarFrame.BorderSizePixel = 0
TabTopBarFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
TabTopBarFrame.Size = UDim2.new(1, 0, 0.1, 0)
TabTopBarFrame.Parent = TabTopBar

local TabTopBarCorner = Instance.new("UICorner")
TabTopBarCorner.CornerRadius = UDim.new(0, 15)
TabTopBarCorner.Parent = TabTopBarFrame

local TabTopBarStroke = Instance.new("UIStroke")
TabTopBarStroke.Thickness = 2
TabTopBarStroke.Color = Color3.fromRGB(85, 73, 255)
TabTopBarStroke.Parent = TabTopBarFrame

-- Tab Title
local TabTitle = Instance.new("TextLabel")
TabTitle.Name = "TabTitle"
TabTitle.TextWrapped = true
TabTitle.BorderSizePixel = 0
TabTitle.TextSize = 16
TabTitle.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
TabTitle.Font = Enum.Font.Jura
