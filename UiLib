local Library = {}
local HiddenGui = get_hidden_gui and get_hidden_gui() or gethui and gethui()

local Colorsdata = {
  BackgroundColor = Color3.fromRGB(0,0,0),
  BorderColor = Color3.fromRGB(0,255,0),
  TextColor = Color3.fromRGB(0,255,0),
}


function Library:SetTheme(data)
  local BackgroundColor = data.BackgroundColor
  local BorderColor = data.BorderColor
  local TextColor = data.TextColor
  
  Colorsdata.BackgroundColor = BackgroundColor
  Colorsdata.BorderColor = BorderColor
  Colorsdata.TextColor = TextColor
end

function Library:CreateWindow(data)
  local Window = {}
  local Title = data.Title
  local TabsDistanceIncrement = data.TabsDistanceIncrement
  
local Gui = Instance.new("ScreenGui")
Gui.Name = " "
Gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
Gui.Parent = HiddenGui

game:GetService("RunService").RenderStepped:Connect(function()
  local BackgroundColor = Colorsdata.BackgroundColor
  local BorderColor = Colorsdata.BorderColor
  local TextColor = Colorsdata.TextColor
  
  for i,v in ipairs(Gui:GetDescendants()) do
    if v:IsA("UIStroke") then
      v.Color = BorderColor
    end
    if v:IsA("Frame") or v:IsA("TextButton") or v:IsA("TextLabel") or v:IsA("TextBox") then
      v.BackgroundColor3 = BackgroundColor
    end
   if v:IsA("TextButton") or v:IsA("TextLabel") or v:IsA("TextBox") then
      v.TextColor3 = TextColor
     end
  end
end)

-- Main Frame
local MainFrame = Instance.new("Frame")
MainFrame.Name = "MainFrame"
MainFrame.ZIndex = 10
MainFrame.BorderSizePixel = 0
MainFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
MainFrame.Size = UDim2.new(1, 0, 1, 0)
MainFrame.BackgroundTransparency = 1
MainFrame.Parent = Gui

-- Others Folder
local Others = Instance.new("Folder")
Others.Name = "Others"
Others.Parent = MainFrame

-- Notify Frame

function Window:CreateNotify(data)
  local Title = data.Title
  local Info = data.Info
  local Duration = data.Duration
  
local Notify = Instance.new("Frame")
Notify.Name = "Notify"
Notify.BorderSizePixel = 0
Notify.BackgroundTransparency = 1
Notify.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Notify.Size = UDim2.new(0.2, 0, 0.3, 0)
Notify.Position = UDim2.new(0.75, 0, 0.65, 0)
Notify.Parent = Others

local NotifyStroke = Instance.new("UIStroke")
NotifyStroke.Thickness = 2
NotifyStroke.Color = Color3.fromRGB(85, 73, 255)
NotifyStroke.Parent = Notify

-- Notify Text
local NotifyText = Instance.new("TextLabel")
NotifyText.Name = "Text"
NotifyText.BorderSizePixel = 0
NotifyText.Text = tostring(Info)
NotifyText.BackgroundTransparency = 1
NotifyText.TextTransparency = 1
NotifyText.TextSize = 16
NotifyText.TextColor3 = Color3.fromRGB(255, 255, 255)
NotifyText.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
NotifyText.Font = Enum.Font.Jura
NotifyText.Size = UDim2.new(1, 0, 0.7, 0)
NotifyText.Position = UDim2.new(0, 0, 0.3, 0)
NotifyText.Parent = Notify

local NotifyTextStroke = Instance.new("UIStroke")
NotifyTextStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
NotifyTextStroke.Thickness = 2
NotifyTextStroke.Color = Color3.fromRGB(85, 73, 255)
NotifyTextStroke.Parent = NotifyText

-- Notify Title
local NotifyTitle = Instance.new("TextLabel")
NotifyTitle.Name = "Title"
NotifyTitle.BorderSizePixel = 0
NotifyTitle.TextSize = 16
NotifyTitle.TextTransparency = 1
NotifyTitle.BackgroundTransparency = 1
NotifyTitle.Text = Title
NotifyTitle.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
NotifyTitle.Font = Enum.Font.Jura
NotifyTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
NotifyTitle.Size = UDim2.new(1, 0, 0.3, 0)
NotifyTitle.Parent = Notify

local NotifyTitleStroke = Instance.new("UIStroke")
NotifyTitleStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
NotifyTitleStroke.Thickness = 2
NotifyTitleStroke.Color = Color3.fromRGB(85, 73, 255)
NotifyTitleStroke.Parent = NotifyTitle


task.spawn(function()
  for i = 1,0,-0.01 do
    Notify.BackgroundTransparency = i
    NotifyText.BackgroundTransparency = i
    NotifyText.TextTransparency = i
    NotifyTitle.BackgroundTransparency = i
    NotifyTitle.TextTransparency = i
    task.wait(0.01)
end
  task.wait(Duration)
  for i = 0,1,0.01 do
    Notify.BackgroundTransparency = i
    NotifyText.BackgroundTransparency = i
    NotifyText.TextTransparency = i
    NotifyTitle.BackgroundTransparency = i
    NotifyTitle.TextTransparency = i
    task.wait(0.01)
  end
  Notify:Destroy()
end)

end


function Window:ShowInfo(data)
  local Title = data.Title
  local Info = data.Info
  local Duration = data.Duration
  
local InfoFrame = Instance.new("Frame")
InfoFrame.Name = "InfoFrame"
InfoFrame.Visible = true
InfoFrame.BorderSizePixel = 0
InfoFrame.BackgroundTransparency = 1
InfoFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
InfoFrame.ZIndex = 100
InfoFrame.AnchorPoint = Vector2.new(0.5, 0.5)
InfoFrame.Size = UDim2.new(0.5, 0, 0.8, 0)
InfoFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
InfoFrame.Parent = Others

local InfoFrameStroke = Instance.new("UIStroke")
InfoFrameStroke.Thickness = 2
InfoFrameStroke.Color = Color3.fromRGB(85, 73, 255)
InfoFrameStroke.Parent = InfoFrame

-- Info Text
local InfoText = Instance.new("TextLabel")
InfoText.Name = "Text"
InfoText.TextWrapped = true
InfoText.BorderSizePixel = 0
InfoText.BackgroundTransparency = 1
InfoText.TextTransparency = 1
InfoText.TextSize = 16
InfoText.TextScaled = true
InfoText.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
InfoText.Font = Enum.Font.Jura
InfoText.TextColor3 = Color3.fromRGB(255, 255, 255)
InfoText.Size = UDim2.new(1, 0, 0.78, 0)
InfoText.Text = tostring(Info)
InfoText.Position = UDim2.new(0, -2, 0.21, 0)
InfoText.Parent = InfoFrame

local InfoTextStroke = Instance.new("UIStroke")
InfoTextStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
InfoTextStroke.Thickness = 2
InfoTextStroke.Color = Color3.fromRGB(85, 73, 255)
InfoTextStroke.Parent = InfoText

-- Info Title
local InfoTitle = Instance.new("TextLabel")
InfoTitle.Name = "Title"
InfoTitle.BorderSizePixel = 0
InfoTitle.TextSize = 32
InfoTitle.BackgroundTransparency = 1
InfoTitle.TextTransparency = 1
InfoTitle.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
InfoTitle.Font = Enum.Font.Jura
InfoTitle.Text = tostring(Title)
InfoTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
InfoTitle.Size = UDim2.new(1, 0, 0.2, 0)
InfoTitle.Parent = InfoFrame

local InfoTitleStroke = Instance.new("UIStroke")
InfoTitleStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
InfoTitleStroke.Thickness = 2
InfoTitleStroke.Color = Color3.fromRGB(85, 73, 255)
InfoTitleStroke.Parent = InfoTitle



task.spawn(function()
  for i = 1,0,-0.01 do
  InfoFrame.BackgroundTransparency = i
  InfoText.BackgroundTransparency = i
InfoText.TextTransparency = i
InfoTitle.BackgroundTransparency = i
InfoTitle.TextTransparency = i
  task.wait(0.01)
end
  task.wait(Duration)
  for i = 0,1,0.01 do
  InfoFrame.BackgroundTransparency = i
  InfoText.BackgroundTransparency = i
InfoText.TextTransparency = i
InfoTitle.BackgroundTransparency = i
InfoTitle.TextTransparency = i
  task.wait(0.01)
end
InfoFrame:Destroy()
end)

end

