local MovementTabButton = Instance.new("TextButton")
MovementTabButton.Name = "Ball"
MovementTabButton.Text = " "
MovementTabButton.BorderSizePixel = 0
MovementTabButton.ZIndex = 100
MovementTabButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
MovementTabButton.AnchorPoint = Vector2.new(0.5, 0.5)
MovementTabButton.Size = UDim2.new(1, 0, 2, 0)
MovementTabButton.Position = UDim2.new(0.5, 0.5, 0, 0)
MovementTabButton.Parent = TabTitle
MovementTabButton.BackgroundTransparency = 1


local Mouse = game:GetService("Players").LocalPlayer:GetMouse()
local Movement = false

function UpdateTabPos(tab)
  task.spawn(function()
    Movement = true
  while Movement do
  local newPos = UDim2.new(0,Mouse.X-110,0,Mouse.Y-40)
  
  tab.Position = newPos
  task.wait(0.001)
  end
  end)
end

MovementTabButton.MouseButton1Down:Connect(function()
  UpdateTabPos(TabTemplate)
end)

game:GetService("UserInputService").InputEnded:Connect(function(input)
  if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
    Movement = false
  end
end)



local OpenCloseTabCorner = Instance.new("UICorner")
OpenCloseTabCorner.CornerRadius = UDim.new(0, 15)
OpenCloseTabCorner.Parent = OpenCloseTabButton




