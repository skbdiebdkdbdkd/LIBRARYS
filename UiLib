OpenButton.MouseButton1Click:Connect(function()
  TopBarFrame.Visible = true
  TabsFrame.Visible = true
  OpenButton.Visible = false
  CloseButton.Visible = true
end)

CloseButton.MouseButton1Click:Connect(function()
  TopBarFrame.Visible = false
  TabsFrame.Visible = false
  CloseButton.Visible = false
  OpenButton.Visible = true
end)

local ConfigOpen = false

ConfigButton.MouseButton1Click:Connect(function()
  ConfigOpen = not ConfigOpen
  ConfigFrame.Visible = ConfigOpen
  
  if ConfigOpen == true then
    ConfigFrame.BackgroundTransparency = 0
   else
     ConfigFrame.BackgroundTransparency = 1
  end
end)

local TabsOffset = 0

function Window:CreateTab(data)
  local Tab = {}
  local Title = data.Title
  
local TabTemplate = Instance.new("Frame")
TabTemplate.Name = "TabTemplate"
TabTemplate.BorderSizePixel = 0
TabTemplate.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
TabTemplate.Size = UDim2.new(0.2, 0, 0.8, 0)
TabTemplate.Position = UDim2.new(TabsOffset, 0, 0.05, 0)
TabTemplate.BackgroundTransparency = 1
TabTemplate.Parent = TabsFrame

TabsOffset = TabsOffset + TabsDistanceIncrement

local TabTemplateCorner = Instance.new("UICorner")
TabTemplateCorner.CornerRadius = UDim.new(0, 15)
TabTemplateCorner.Parent = TabTemplate

local TabTopBar = Instance.new("Folder")
TabTopBar.Name = "TabTopBar"
TabTopBar.Parent = TabTemplate

-- Tab Top Bar Frame
local TabTopBarFrame = Instance.new("Frame")
TabTopBarFrame.Name = "TabTopBarFrame"
